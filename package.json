{
  "name": "typescript-logging",
  "version": "0.2.0",
  "description": "Library for logging, written in typescript, but can be used by normal es5+ javascript as well.",
  "main": "dist/commonjs/typescript-logging",
  "scripts": {
    "test": "tsc -p tsconfig-test.json && jasmine-node --verbose ./dist-test/spec",
    "tslint": "tslint -c tslint.json -p tsconfig.json src/logging/**",
    "build": "tsc && npm run tslint && webpack --config webpack.config.bundle.js && webpack --config webpack.config.bundle-min.js",
    "docs": "typedoc --tsconfig tsconfig-docs.json --excludeNotExported --excludePrivate --mode modules --out dist/docs/api src/logging/typescript-logging.ts",
    "release": "npm run build && npm run test && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mreuvers/typescript-logging.git"
  },
  "keywords": [
    "typescript-logging",
    "logging",
    "javascript-logging",
    "log",
    "logger"
  ],
  "author": "Martijn Reuvers <martijnr.work@gmail.com> (http://www.javalid.org)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mreuvers/typescript-logging/issues"
  },
  "homepage": "https://github.com/mreuvers/typescript-logging#readme",
  "devDependencies": {
    "jasmine": "2.5.2",
    "jasmine-node": "1.14.5",
    "source-map-loader": "0.1.5",
    "ts-loader": "1.3.2",
    "tslint": "4.1.1",
    "typedoc": "0.5.1",
    "typescript": "2.1.1",
    "webpack": "1.14.0"
  },
  "typings": "./dist/commonjs/typescript-logging.d.ts",
  "dependencies": {
    "@types/es6-promise": "0.0.32",
    "@types/jasmine-node": "1.14.30",
    "@types/stacktrace-js": "0.0.30",
    "stacktrace-js": "1.3.1"
  }
}
