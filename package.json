{
  "name": "typescript-logging",
  "version": "0.2.0",
  "description": "Library for logging, written in typescript, but can be used by normal es5+ javascript as well.",
  "main": "dist/commonjs/typescript-logging",
  "scripts": {
    "setup": "typings install",
    "test": "tsc -p tsconfig-test.json && jasmine-node --verbose ./dist-test/spec",
    "build": "tsc && webpack --config webpack.config.bundle.js && webpack --config webpack.config.bundle-min.js && npm run extension",
    "extension": "node build-extensions.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mreuvers/typescript-logging.git"
  },
  "keywords": [
    "typescript-logging",
    "logging",
    "javascript-logging",
    "log",
    "logger"
  ],
  "author": "Martijn Reuvers <martijnr.work@gmail.com> (http://www.javalid.org)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mreuvers/typescript-logging/issues"
  },
  "homepage": "https://github.com/mreuvers/typescript-logging#readme",
  "devDependencies": {
    "jasmine": "^2.4.1",
    "jasmine-node": "^1.14.5",
    "shelljs": "^0.7.4",
    "typescript": "^2.0.0",
    "typings": "^1.3.2",
    "webpack": "^1.13.2"
  },
  "typings": "./dist/commonjs/typescript-logging.d.ts",
  "dependencies": {
    "stacktrace-js": "^1.3.1"
  }
}
